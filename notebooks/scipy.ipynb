    "x = np.linspace(0, 4, 12)\n",
    "\n",
    "y = np.cos(x**2/3+4)\n",
    "\n",
    "f1 = interpolate.interp1d(x, y,kind = 'linear')\n",
    "f2 = interpolate.interp1d(x, y, kind = 'cubic')\n",
    "\n",
    "xnew = np.linspace(0, 4,30)\n",
    "\n",
    "new_values1 = f1(xnew)"
